# .env 文件模板
# 从 my.telegram.org 获取
API_ID=12345678
API_HASH=your_api_hash_string
# 您的Telegram账号信息
PHONE=+1234567890
# 如果您开启了二次验证（Two-Factor Authentication），请填写此项，否则留空
PASSWORD=your_2fa_password

# 填入要转发的所有频道id
#CHANNEL_IDENTIFIERS=
CHANNEL_IDS=1234579999
# 目标频道：填写您要将消息转发到的频道的用户名
DESTINATION_CHANNEL=my_destination_channel_username

# --- 关键词过滤配置 ---
KEYWORD_BLACKLIST=黑马程序员,epub,#带货,尚硅谷,去头去尾,C4D,#PanWEB,#游戏,#有声书

# --- 链接去重与清理配置 ---
# 是否开启基于夸克链接的去重功能 (true/false)
DEDUPLICATION_ENABLED=true
# (此项已弃用，但保留以兼容旧版)
# DEDUPLICATION_CHAR_COUNT=52
# 去重时，检查的目标频道最近消息数量
DEDUPLICATION_CACHE_SIZE=200
